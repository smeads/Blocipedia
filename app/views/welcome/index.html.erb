<h1>Welcome to Blocipedia</h1>
  <% if current_user %>
    <p>
        <%= link_to "Wikis", wikis_path, class: 'btn btn-primary' %>
  <% else %>
    <p>
        <%= link_to "Sign In", new_user_session_path, class: 'btn btn-primary' %> or
        <%= link_to "Sign Up", new_user_registration_path, class: 'btn btn-primary' %>
    </p>
  <% end %>
